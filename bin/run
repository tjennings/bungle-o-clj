#!/usr/bin/env bash
CLASSPATH=classes:src

base=`dirname $0`

pushd $base/.. > /dev/null

for f in lib/*.jar; do
    CLASSPATH=$CLASSPATH:$f
done

for f in lib/*/*.jar; do
    CLASSPATH=$CLASSPATH:$f
done

java -client -Xmx3000m -cp $CLASSPATH clojure.main src/bungle_o/thread_study.clj $@ <&0

popd > /dev/null
